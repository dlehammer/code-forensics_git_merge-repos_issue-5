<plugin name='quartz' version='1.0-RC2' grailsVersion='2.2.0 &gt; *'>
  <author>Sergey Nebolsin, Graeme Rocher</author>
  <authorEmail>nebolsin@gmail.com</authorEmail>
  <title>Quartz plugin for Grails</title>
  <description>This plugin adds Quartz job scheduling features to Grails application.
</description>
  <documentation>http://grails.org/plugin/quartz</documentation>
  <type>QuartzGrailsPlugin</type>
  <resources>
    <resource>DefaultQuartzConfig</resource>
    <resource>QuartzBootStrap</resource>
    <resource>grails.plugins.quartz.JobManagerService</resource>
  </resources>
  <repositories>
    <repository name='http://coco-php:8080/artifactory/remote-repos' url='http://coco-php:8080/artifactory/remote-repos/' />
    <repository name='http://coco-php:8080/artifactory/libs-release' url='http://coco-php:8080/artifactory/libs-release/' />
    <repository name='http://coco-php:8080/artifactory/libs-snapshot' url='http://coco-php:8080/artifactory/libs-snapshot/' />
    <repository name='http://coco-php:8080/artifactory/plugins-release' url='http://coco-php:8080/artifactory/plugins-release/' />
    <repository name='http://coco-php:8080/artifactory/plugins-snapshot' url='http://coco-php:8080/artifactory/plugins-snapshot/' />
    <repository name='http://coco-php:8080/artifactory/plugins-release-local' url='http://coco-php:8080/artifactory/plugins-release-local/' />
    <repository name='http://coco-php:8080/artifactory/plugins-snapshot-local' url='http://coco-php:8080/artifactory/plugins-snapshot-local/' />
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='Grails Snapshots' url='http://snapshots.repository.codehaus.org/' />
    <repository name='Spring Milestone' url='http://maven.springframework.org/milestone/' />
    <repository name='ebrRelease' url='http://repository.springsource.com/maven/bundles/release/' />
    <repository name='ebrExternal' url='http://repository.springsource.com/maven/bundles/external/' />
    <repository name='http://download.java.net/maven/2/' url='http://download.java.net/maven/2/' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='com.amazonaws' name='aws-java-sdk' version='1.3.30' />
    </compile>
    <runtime>
      <dependency group='org.apache.ant' name='ant-nodeps' version='1.8.1' />
      <dependency group='org.apache.ant' name='ant-trax' version='1.8.0' />
      <dependency group='org.apache.ant' name='ant-launcher' version='1.8.1' />
      <dependency group='mysql' name='mysql-connector-java' version='5.1.18' />
      <dependency group='org.apache.ant' name='ant-junit' version='1.8.1' />
    </runtime>
  </dependencies>
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='console' version='1.1' />
      <plugin group='org.grails.plugins' name='svn' version='1.0.2' />
      <plugin group='org.grails.plugins' name='mail' version='1.0.1' />
      <plugin group='org.grails.plugins' name='rest' version='0.7' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='hibernate' version='2.2.0' />
    </runtime>
    <build>
      <plugin group='org.grails.plugins' name='tomcat' version='2.2.0' />
    </build>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>